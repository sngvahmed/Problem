#include "bits/stdc++.h"
#include <functional>
#include <ext/numeric> 
using namespace std;

#define SZ(x) (int)x.size()
#define pb push_back
#define mp make_pair
#define F first
#define S second
#define all(x) x.begin(), x.end()
#define rall(x) x.rbegin(), x.rend()
#define MMS(x, v) memset(x, v, sizeof(x))
#define SF(x) scanf("%d",&x)

int main() {
	int n, m;
	SF(n), SF(m);
	map<int, int> vis;
	int ini = 1, i = 0, s = 1;
	while (true) {
		if (m == 0)
			cout << ini << " ";
		if ((ini + ((i % m) * s)) <= n && vis[ini + ((i % m) * s)] == 0) {
			ini += (i % m);
		}
		if (ini == n)
			s = -1;

		m--;
	}
	return 0;
}
