#include "bits/stdc++.h"
#include <functional>
#include <ext/numeric> 
using namespace std;

#define SZ(x) (int)x.size()
#define pb push_back
#define mp make_pair
#define F first
#define S second
#define all(x) x.begin(), x.end()
#define rall(x) x.rbegin(), x.rend()
#define MMS(x, v) memset(x, v, sizeof(x))
#define SF(x) scanf("%d" , &x)

int main() {
	int n, m;
	SF(n), SF(m);
	int a = n, b = n - m, rem = n - b;
	while (n--) {
		cout << n < " ";
	}
	while (a <= b) {
		if (a == b)
			cout << a;
		else
			cout << a++ << " " << b++ << " ";
	}
	return 0;
}
