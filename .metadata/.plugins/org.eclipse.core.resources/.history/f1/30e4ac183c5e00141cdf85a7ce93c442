#include "bits/stdc++.h"
#include <functional>
#include <ext/numeric> 
using namespace std;

#define SZ(x) (int)x.size()
#define pb push_back
#define mp make_pair
#define F first
#define S second
#define all(x) x.begin(), x.end()
#define rall(x) x.rbegin(), x.rend()
#define MMS(x, v) memset(x, v, sizeof(x))
#define SF(x) scanf("%d" , &x)

long long PowerMod(int x, int p, int m) {
	if (!p)
		return 1;
	long long y = PowerMod(x, p >> 1, m);
	y = y % m;
	y = (y * y) % m;
	if (p & 1)
		y = (y * x) % m;
	return y;
}

long long modInv(int x, int m) {
	if (__gcd(x, m) != 1)
		throw string("not coprimes ya 7aywan");
	return PowerMod(x, m - 2, m);
}

bool coprime(long long i, long long j, long long k) {

	return true;
}
int main() {
	long long st, end;
	cin >> st >> end;
	for (long long i = st; i <= end; i++) {
		for (long long j = i + 1; j <= end; j++) {
			for (long long k = j + 1; k <= end; k++) {
				if (coprime(i, j, k))
					return 0;
			}
		}
	}
	cout << -1 << endl;
	return 0;
}
